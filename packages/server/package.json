{
  "name": "@holochain/create",
  "version": "0.0.1",
  "description": "RAD tools for quickly scaffolding holochain apps",
  "author": "guillem.cordoba@gmail.com",
  "scripts": {
    "start": "npm run build && concurrently \"webpack --config webpack.config.js --watch\" \"nodemon --config ./nodemon.config.json\" --raw",
    "build": "rm -rf dist && webpack --config webpack.config.js",
    "publish-to-branch": "npm run build && gh-pages -d ./ -b build"
  },
  "bin": "dist/app.js",
  "dependencies": {
    "@mdi/font": "5.9.55",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "diff": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "open": "^8.2.1",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^6.4.1",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^3.2.3",
    "handlebars-loader": "^1.7.1",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "~1.32",
    "sass-loader": "^10.0.0",
    "stream": "^0.0.2",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.3",
    "webpack": "^4.36.0",
    "webpack-cli": "^4.8.0"
  }
}
