{
  "name": "ui",
  "version": "0.1.0",
  "scripts": {
    "start": "vite --port $UI_PORT --clearScreen false",
    "build": "{{(package_manager_command "check" null)}} && vite build",
    "check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "package": "{{(package_manager_command "build" null)}} && rimraf dist.zip && cd dist && bestzip ../dist.zip *"
  },
  "dependencies": {
    "@holochain/client": "{{holochain_client_version}}",
    "@msgpack/msgpack": "^2.8.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "bestzip": "^2.2.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.6.3",
    "vite": "^6.2.5",
    "vue-tsc": "^2.1.8"
  },
  "type": "module"
}