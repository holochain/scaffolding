<label htmlFor="{{label}}">{{label}}</label>
{{#if (eq cardinality "vector")}}

<input
{{#if key}}
  key={ {{key}} }
{{/if}}
  type="checkbox"
  name="{{label}}"
{{#if variable_to_read}}
  checked={ {{variable_to_read}} }
{{/if}}
  onChange={(e) => {
    set{{pascal_case variable_to_change}}(prevValues => prevValues?.map((v, idx) => {
      if ({{key}} === idx) {
        v = e.target.checked;
      }
      return v;
    }))
  }}
/>
{{else}}

<input type="checkbox" name="{{label}}" checked={ {{variable_to_read}} } onChange={(e) => set{{pascal_case variable_to_change}}(e.target.checked)} />
{{/if}}