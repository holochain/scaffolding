  <mwc-list-item {{#if variable_to_read}}.selected=${ {{variable_to_read}} === '{{pascal_case this}}' }{{/if}} @request-selected=${() => { {{variable_to_change}} = '{{pascal_case this}}'; } }>{{title_case this}}</mwc-list-item>
{{/each}}
</mwc-formfield>
